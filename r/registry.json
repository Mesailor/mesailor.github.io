{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "shadcn-chat",
  "homepage": "https://mesailor.github.io",
  "items": [
    {
      "name": "chat",
      "type": "registry:component",
      "title": "Chat",
      "description": "A set of composable chat UI components with container query support.",
      "registryDependencies": ["avatar", "button", "textarea"],
      "dependencies": ["@radix-ui/react-avatar"],
      "files": [
        {
          "path": "registry/new-york/chat/chat.tsx",
          "type": "registry:file",
          "target": "components/chat/chat.tsx"
        },
        {
          "path": "registry/new-york/chat/chat-header.tsx",
          "type": "registry:file",
          "target": "components/chat/chat-header.tsx"
        },
        {
          "path": "registry/new-york/chat/chat-messages.tsx",
          "type": "registry:file",
          "target": "components/chat/chat-messages.tsx"
        },
        {
          "path": "registry/new-york/chat/chat-toolbar.tsx",
          "type": "registry:file",
          "target": "components/chat/chat-toolbar.tsx"
        },
        {
          "path": "registry/new-york/chat/chat-event.tsx",
          "type": "registry:file",
          "target": "components/chat/chat-event.tsx"
        }
      ]
    },
    {
      "name": "open-in-v0-basic",
      "type": "registry:block",
      "title": "Open in v0 Basic",
      "description": "A basic chat example block for the Open in v0 button.",
      "registryDependencies": [
        "https://shadcn-chat.vercel.app/r/chat.json",
        "separator",
        "input-group"
      ],
      "dependencies": [
        "@radix-ui/react-separator",
        "@radix-ui/react-slot",
        "class-variance-authority"
      ],
      "files": [
        {
          "path": "registry/new-york/blocks/open-in-v0-basic/page.tsx",
          "type": "registry:page",
          "target": "app/page.tsx"
        },
        {
          "path": "registry/new-york/blocks/open-in-v0-basic/components/primary-message.tsx",
          "type": "registry:component",
          "target": "components/examples/primary-message.tsx"
        },
        {
          "path": "registry/new-york/blocks/open-in-v0-basic/components/additional-message.tsx",
          "type": "registry:component",
          "target": "components/examples/additional-message.tsx"
        },
        {
          "path": "registry/new-york/blocks/open-in-v0-basic/components/date-item.tsx",
          "type": "registry:component",
          "target": "components/examples/date-item.tsx"
        },
        {
          "path": "registry/new-york/blocks/open-in-v0-basic/data/messages.ts",
          "type": "registry:file",
          "target": "data/examples/messages.ts"
        }
      ]
    }
  ]
}
